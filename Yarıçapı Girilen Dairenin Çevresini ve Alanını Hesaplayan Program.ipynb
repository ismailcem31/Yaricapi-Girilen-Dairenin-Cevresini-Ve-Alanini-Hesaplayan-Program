   "source": [
    "# Pi sayısını tanımla\n",
    "pi = 3.14159\n",
    " \n",
    "# Kullanıcıdan yarıçapı al\n",
    "yaricap = float(input(\"Lütfen dairenin yarıçapını girin: \"))\n",
    " \n",
    "# Çevreyi hesapla\n",
    "cevre = 2 * pi * yaricap\n",
    " \n",
    "# Alanı hesapla\n",
    "alan = pi * yaricap**2\n",
    " \n",
    "print(\"Dairenin Çevresi:\", cevre)\n",
    "print(\"Dairenin Alanı:\", alan)"
   ]
  }
